{
    "manifest_version": 2,
    "name": "kellyface everywhar",
    "version": "100.100",
    "description": "sets the background of every element to kelly's face",
    "content_scripts": 
    [
        {
            "matches": ["*://*/*"],
            "css": ["kellyface.css"],
            "js": ["content_script.js"],
            "run_at": "document_end"
        }
    ]
}

